<!DOCTYPE html>
<html>

<body>
    <ul class="std_tbl">
        <% for(var i=0; i<std_tbl.length; i++) {%>
        <li class="std">
            <span><%= std_tbl[i].name %></span>
            <span><%= std_tbl[i].id %></span>
        </li>
        <% } %>
    </ul>
    <form action="/api/post" method="POST">
        <input type="number" placeholder="id" name="std_id"><br>
        <input type="text" placeholder="name" name="name"><br>
        <button type="submit">submit</button>
    </form>
    <script src="main.js"></script>
</body>

</html>